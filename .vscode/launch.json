    "version": "0.2.0",
    "compounds": [
        {
            "name": "All_PROD",
            "configurations": ["Android-PROD", "iPhone-PROD"],
        },
        {
            "name": "All_DEV",
            "configurations": ["Android-DEV", "iPhone-DEV"],
        }
    ],
    "configurations": [
        {
            "name": "Android-PROD",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_prod.dart",
            "deviceId": "android",
            "args": [
                "--target",
                "lib/main_prod.dart"
            ]
        },
        {
            "name": "iPhone-PROD",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_prod.dart",
            "deviceId": "iphone",
            "args": [
                "--target",
                "lib/main_prod.dart"
            ]
        },
        {
            "name": "Android-DEV",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_dev.dart",
            "deviceId": "android",
            "args": [
                "--target",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "iPhone-DEV",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main_dev.dart",
            "deviceId": "iphone",
            "args": [
                "--target",
                "lib/main_dev.dart"
            ]
        },
